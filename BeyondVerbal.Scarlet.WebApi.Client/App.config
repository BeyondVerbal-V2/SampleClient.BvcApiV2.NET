<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="clientOptions" type="BeyondVerbal.Scarlet.WebApi.Client.Options,BeyondVerbal.Scarlet.WebApi.Client"/>


  </configSections>

  <clientOptions
    apiKey="YOUR-API-KEY"
    serverUrl ="https://beta.beyondverbal.com//"
    soundFile="WAVE-FILE.WAV"
    outputFile="dump.{0}.log"
    filenameIsTemplate="true"
    bytePerSecRate="16000"
  />

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>